type Usercompanies {
	usercompid: Userregistration
	userregistration: String!
	companyname: String!
	doj: String!
	dor: String!
	noticeperiod: String!
	documents: String!
	achievementdesr: String!
	salary: String!
}

extend type Query { 
	getCompanies(username: String!): [Usercompanies]!
}

extend type Mutation { 
	addUpdateCompanies(Usercompanies: Usercompanies): Boolean
}